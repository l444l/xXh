for (const [featureName, feature] of Object.entries(features)) {
  if(feature.triggers?.includes(isCommand)) {
    try {
      await feature.handle(KNTL, kntl, m, text, command, isCommand);
      break;
    } catch (error) {
      console.error(`Error handling command with feature ${featureName}:`, error);
    }
  }
}
